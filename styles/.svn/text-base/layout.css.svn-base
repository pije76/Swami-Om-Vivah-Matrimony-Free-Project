@import url(navi.css);
@import url(forms.css);

body{
    margin:0;
    padding:0;
    font-size:12px;
    font-family:verdana, Arial, Helvetica, sans-serif;
    color:#FFFFFF;
    background-color:#29313C;
}

img{border:0; display:block; padding:0; margin:0;}
.spaces{clear: both;}
br.spaces{clear:both; margin-top:-15px;}
a{outline:none; text-decoration:none; color:gray;}
a:hover {color:#C00000;}

.divFloatLeft
{
    float:left;
}

.divFloatRight
{
    float:right;
}

.profileImage{border:1px solid #DFDFDF; padding:5px; color:#666666; background-color:#FFFFFF;}
.profileImage{float:left; margin:0 8px 8px 0; clear:left;}

div.box{
    display:block;
    width:100%;
    margin:0;
    text-align:left;
}

div.box h1, div.box h2, div.box h3, div.box h4, div.box h5, div.box h6{
    margin:0 0 20px 0;
    padding:0 0 8px 0;
    font-size:20px;
    font-weight:normal;
    font-family:Georgia, "Times New Roman", Times, serif;
    border-bottom:1px dotted #DDDDDD;
}

.headerDiv{color:#D55600; background: url("../images/gradient.jpg") repeat scroll 0 0;}
.menus{color:#FFFFFF; background-color:#FEAA55;}
.contents{color:#666666; background: url("../images/gradient.jpg") repeat scroll 0 0;}

#header, #menusbar, #mainContent, #services, #breadcrumb, #container, #footer, #copyright{
    position:relative;
    margin:0 auto 0;
    display:block;
    width:960px;
}

#header{
    padding:30px 0;
    font-family:Georgia, "Times New Roman", Times, serif;
}

#header #logoNmoto{
    display:block;
    float:left;
    width:355px;
    margin-top:7px;
}

#header #logoNmoto h1, #header #logoNmoto p{
    margin:0;
    padding:0;
    line-height:normal;
}

#header #logoNmoto h1{
    margin:0 0 10px 0;
    padding:0;
    font-size:36px;
    border:none;
}

#logoImage{
    width: 100px;
    float: left;
    height: 76px;
}	

#header h1 a{
    color:#C00000;
}

#header #loginDiv{
    background-color: #FEAA55;
    color: #000000;
    float: right;
    padding: 10px 10px 12px;
    width: 362px;
    height: 90px;
}

#header #loginDiv p{
    margin:0 0 8px 0;
    font-family:Verdana, Arial, Helvetica, sans-serif;
}

#header #loginDiv a{
    color:black;
}	

#header #loginDiv a:hover{
    color:#c00000;
}		


#menusbar #search{
    display:block;
    float:right;
    width:242px;
    height:36px;
    margin:0;
    padding:10px 0 0 17px;
    color:#FFFFFF;
    background-color:#FEAA55;
    border-left:1px dotted #666666;
    overflow:hidden;
}

#mainContent{
    padding:22px 0 5px 0;
    font-size:16px;
    font-family:Georgia, "Times New Roman", Times, serif;
}

#mainContent .divFloatLeft{
    display:block;
    float:left;
    width:560px;
    height:300px;
    margin:0;
    color:#FFFFFF;
}

#mainContent .divFloatLeft h2{
    font-size:46px;
    margin:0 0 15px 0;
    padding:0;
    border:none;
    color:#FFFFFF;
    background-color:#2684B7;
}

#mainContent .divFloatLeft p{
    margin:0;
    padding:0;
    line-height:1.6em;
}

#mainContent .divFloatLeft p.readmore{
    display:block;
    width:100%;
    margin:20px 0 0 0;
    padding:0;
    text-align:right;
    line-height:normal;
}

#mainContent .divFloatLeft p.readmore a{
    padding:8px 15px;
    font-size:18px;
    color:#FFFFFF;
    background-color:#1C5E82;
}

#mainContent .divFloatRight{

    background-color: #FEAA55;
    float: right;
    height: 232px;
    padding: 34px;
    width: 319px; 

}

#services ul, #services p, #services h2, #services img{
    margin:0;
    padding:0;
    list-style:none;
}

#services li{
    display:block;
    float:left;
    width:290px;
    border: 2px solid #FF6E31;
    height:195px;
    margin:0 9px 0 0;
    padding:10px;
    color:#666666;
    background: url("../images/gradient2.jpg") repeat scroll 0 0;
}


#services li.last{
    margin-right:0;
}

#services .divFloatRight{
    display:block;
    float:right;
    width:285px;
    padding:10px 0 0 0;
}

#services h2{
    margin:0 0 5px 0;
    padding:0 0 3px 0;
    font-size:14px;
    font-weight:bold;
    color:#097054;
    border-bottom-color:#666666;
}

#services p{
    margin:0;
    padding:0;
    line-height:1.6em;
    font-size: 13px;
    font-weight: bolder;
    color: crimson;
}

#services p.readmore{
    display:block;
    width:100%;
    margin:15px 0 0 0;
    padding:0;
    text-align:right;
    line-height:normal;
}

#services p.readmore a{
    padding:8px 15px;
    font-weight:bold;
    color:#FFFFFF;
    background-color:#C00000;
}

#container{
    padding:39px 0 3px;
    line-height:1.4em;
}

#container a{
    color:#C00000;
}

#container a:hover{
    color:#ffffff;
}	

#container h1, #container h2, #container h3, #container h4, #container h5, #container h6{
    padding-bottom:8px;
    border-bottom:1px dotted #CCCCCC;
    color:#C00000;
}

#content{
    display:block;
    float:left;
    width:600px;
}

#newProfiles{
    display:block;
    float:right;
    width:300px;
}

#newProfiles .profileList, #newProfiles #featured{
    display:block;
    width:300px;
    margin-bottom:20px;
}

#newProfiles .profileList h2.title{
    display:block;
    width:100%;
    height:65px;
    margin:0;
    padding:15px 0 0 0;
    font-size:20px;
    line-height:normal;
    border-bottom:1px dashed #666666;
}

#newProfiles .profileList h2.title img{
    float:left;
    margin:-15px 8px 0 0;
    padding:5px;
    border:1px solid #666666;
}

#newProfiles div.imgholder{
    display:block;
    width:290px;
    margin:0 0 10px 0;
    padding:4px;
    color:#666666;
    background-color:#FFFFFF;
    border:1px solid #CCCCCC;
}

#newProfiles .profileList p.readmore{
    display:block;
    width:100%;
    font-weight:bold;
    text-align:right;
    line-height:normal;
}


#newProfiles #latestProfiles{
    display:block;
    width:100%;
    margin:0;
    padding:0;
    list-style:none;
}

#newProfiles #latestProfiles li{
    display:block;
    margin:0 0 20px 0;
    padding:0 0 15px 0;
    border-bottom:1px dotted #DDDDDD;
}

#newProfiles #latestProfiles li.last{
    margin-bottom:0;
}

#newProfiles #latestProfiles p{
    margin:0;
    padding:0;
    line-height:normal;
}

#newProfiles #latestProfiles .profileImage{
    margin:0 10px 10px 0;
    padding:4px;
}

#copyright{
    padding:20px 0;
}

#copyright p{
    margin:0;
    padding:0;
}

#copyright ul
{
    margin: 0;
    padding: 0;
    list-style-type: none;
}

#copyright ul li { display: inline; }	

#androidApp {
    background-color: #FFFFFF;
    border: 3px outset #C00000;
    color: #C00000;
    font-size: 15px;
    height: 239px;
    padding: 10px 10px 10px 2px;
    width: 107px;
}	

#androidApp a{
    color:blueviolet;
}	
.pageLoadingOverlay {
    height: 100%;
    width: 100%;
    display: block;
    z-index: 100;
    position: absolute;
    left: 0;
    top: 0;
    background: rgba(0, 0, 0, 0.5) url("../images/ajax-loader.gif") no-repeat center center; }

.pageOverlay {
    opacity : 0.4;
    background-color: black;
    height: 100%;
    width: 100%;
    display: block;
    z-index: 100;
    position: absolute;
    left: 0;
    top: 0;
}
/*_______________________________Slideshow_____________________________________*/

#slideshow { 
    position: relative; 
    width: 560px; 
    height: 300px; 
    box-shadow: 0px 0px 22px #4D5564;
   -webkit-box-shadow: 0px 0px 22px #4D5564;
   -moz-box-shadow: 0px 0px 22px #4D5564;
    border-collapse: separate;
}

#slideshow img{
    height:280px;
    width:540px;
}

#slideshow > div { 
    position: absolute; 
    top: 10px; 
    left: 10px; 
    right: 10px; 
    bottom: 10px; 
}

